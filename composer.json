{
    "name": "tankfairies/tcrypt",
    "description": "End to end encryption hanlder",
    "type": "library",
    "keywords": ["encryption", "sodium"],
    "homepage": "https://tankfairies.com/",
    "license": "MIT",
    "authors": [
        {
            "name": "Jon",
            "email": "jon@tankfairies.com",
            "role": "lead"
        }
    ],
    "support": {
        "issues": "https://github.com/tankfairies/tcrypt/issues"
    },
    "prefer-stable": true,
    "require": {
        "php": "^8.2",
        "ext-sodium": "*"
    },
    "autoload": {
        "psr-4": {
            "Tankfairies\\Tcrypt\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "codeception/codeception": "^5.1",
        "codeception/module-phpbrowser": "^3.0",
        "codeception/module-asserts": "^3.0"
    },
    "config": {
        "platform": {
            "php": "8.2"
        },
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
